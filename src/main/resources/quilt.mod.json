{
    "schema_version": 1,
    "quilt_loader": {
        "group": "${mod_group}",
        "id": "${mod_id}",
        "version": "${mod_version}",
        "intermediate_mappings": "net.fabricmc:intermediary",
        "metadata": {
            "name": "${mod_name}",
            "description": "A short description of your mod.",
            "contributors": {
                "TheEntropyShard": "Owner"
            },
            "contact": {
                "homepage": "https://github.com/TheEntropyShard/DynamicTitle",
                "issues": "https://github.com/TheEntropyShard/DynamicTitle/issues",
                "sources": "https://github.com/TheEntropyShard/DynamicTitle"
            },
            "icon": "assets/dynamictitle/icon.png"
        },
        "entrypoints": {
            "init": "me.theentropyshard.dynamictitle.DynamicTitleMod"
        },
        "depends": [
            {
                "id": "cosmicquilt",
                "versions": "*"
            },
            {
                "id": "cosmicreach",
                "versions": "*"
            }
        ]
    },
    "mixin": "dynamictitle.mixins.json"
}
